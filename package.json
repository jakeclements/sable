    "angular": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "ng-redux": "^3.3.3",
    "redux": "^3.5.2",
    "redux-ui-router": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
